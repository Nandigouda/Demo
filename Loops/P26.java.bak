class P26 
{
	public static void main(String[] args) 
	{
		int n=5;
	int a=1;
		char ch='A';
		for (int row=0;row<n;row++)
		{
			for (int col=0;col<n;col++)
			{
				if(row==n/2||col==n/2)
				{
				if (row%2==1)
					{
		System.out.print(a+" ");
		a++;
					}	
		else
			
		System.out.print(ch+" ");
				ch++;
				}
		else
			
				System.out.print("  ");
			}
							System.out.println();
		}}}

	