class p7 
{
	public static void main(String[] args) 
	{
		int n=4; int m=4;
	char ch='A';
	int b=1;
		for(char row=1;row<=m;row++)
		{
			for(char col=1;col<=n;col++)
			{
			if (col%2==0)
				{
				System.out.print(ch+" ");
				ch+=2;
				}		
				else if(row%2==1)
				{
					System.out.print(b);
					b+=2;
				}
             		System.out.println();
		}
	}}}