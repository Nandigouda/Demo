/*
A B C D
1 3 5 7
A B C D
1 3 5 7
*/



class  forloop2
{
	public static void main(String[] args) 
	{
		int n=4;
		for(int row=1;row<=n;row++)
		{
			char ch='A';
	    int a=1;
			for(int col=1;col<=n;col++)
			    {
				if (row%2==0)
				{
				System.out.print(a+" ");
				a+=2;
				}
				else
				{
				System.out.print(ch+" ");
				ch++;
				}}
             	System.out.println();
		}
	}
}

