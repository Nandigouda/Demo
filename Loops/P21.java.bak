class P21 
{
	public static void main(String[] args) 
	{
		int n=5;
		for (int row=0;row<n;row++)
		{
			for (int col=0;col<n;col++)
			{
				if (row==n/2||col==n/2||row==0&&col<=n/2||col==0&&row>=n/2||col==n-1&&row<=n/2||row==n-1&&row>=n/2||row==4&&col==1&&row==4&&col==3)
				{
		System.out.print("* ");
				}
				else
				{
							System.out.print("  ");
			}
		}
		System.out.println();
	}
}
}